package com.blackboard.classin.entity;


public class ServerConfigForm   {

	private static final long serialVersionUID = 1020612304673197275L;

	private String server_address = null;

	private String server_name = null;

	private String server_id = null;

	private String server_password = null;

	private String datasource = null;

	private String describsion = null;

	public String getServer_address() {
		return server_address;
	}

	public void setServer_address(String server_address) {
		this.server_address = server_address;
	}

	public String getServer_id() {
		return server_id;
	}

	public void setServer_id(String server_id) {
		this.server_id = server_id;
	}

	public String getServer_password() {
		return server_password;
	}

	public void setServer_password(String server_password) {
		this.server_password = server_password;
	}

	public String getDatasource() {
		return datasource;
	}

	public void setDatasource(String datasource) {
		this.datasource = datasource;
	}

	public String getDescribsion() {
		return describsion;
	}

	public void setDescribsion(String describsion) {
		this.describsion = describsion;
	}

	public String getServer_name() {
		return server_name;
	}

	public void setServer_name(String server_name) {
		this.server_name = server_name;
	}

}