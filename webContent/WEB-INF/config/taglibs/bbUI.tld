    </description>
    <attribute>
      <name>formName</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The name of the form enclosing the the userId textfield</description>
    </attribute>
    <attribute>
      <name>username</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The initial value of the username textfield</description>
    </attribute>
    <attribute>
      <name>textFieldName</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The name of the text field for the userId</description>
    </attribute>
    <attribute>
      <name>textFieldSize</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The size of the text field for the userId</description>
    </attribute>
    <attribute>
      <name>selectMultiple</name>
