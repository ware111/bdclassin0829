<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="bbUI.xsl"?>

<!DOCTYPE taglib
 PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
 "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>

  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>bbUI</short-name>
  <uri>/bbUI</uri>
  <description>
    The Blackboard User Interface tag library encapsulates Blackboard user interface
    elements to allow developers easy access to the Blackboard look and feel
  </description>

  <tag>
    <name>loginWelcome</name>
    <tag-class>blackboard.servlet.tags.LoginWelcomeTag</tag-class>
    <body-content>empty</body-content>
    <description>Creates the standard login welcome panel.</description>
    <attribute>
      <name>errorMessage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Error message from previous login attempt</description>
    </attribute>
  </tag>

  <tag>
    <name>loginForm</name>
    <tag-class>blackboard.servlet.tags.LoginFormTag</tag-class>
    <body-content>empty</body-content>
    <description>
      Creates the standard login form ( same look and feel as rendered by login.pl )
    </description>
    <attribute>
      <name>validateFormScript</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Name of the inline script which will validate the login form
      </description>
    </attribute>
    <attribute>
      <name>errorMessage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Error message from previous login attempt</description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The string to use as the value for the target attribute in the login form. Default
        is none, which should be used for all relogins except when a guest logs in as a
        real user
      </description>
    </attribute>
  </tag>

  <tag>
    <name>createAccountForm</name>
    <tag-class>blackboard.servlet.tags.CreateAccountFormTag</tag-class>
    <body-content>empty</body-content>
    <description>Creates the create-account form.</description>
    <attribute>
      <name>createAccountLink</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Name of the page that will create a new user account</description>
    </attribute>
  </tag>

  <tag>
    <name>copyright</name>
    <tag-class>blackboard.servlet.tags.CopyrightTag</tag-class>
    <body-content>empty</body-content>
    <description>Standard HTML snippet for Blackboard copyright notice</description>
    <attribute>
      <name>type</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Type of copyright notice. Set to "footer" to display the footer image. Any other
        values ignored
      </description>
    </attribute>
    <attribute>
      <name>showFooterImage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        This attribute is added in case the footer needs to be displayed without the image
      </description>
    </attribute>
  </tag>

  <tag>
    <name>validateLoginFormScript</name>
    <tag-class>blackboard.servlet.tags.ValidateLoginFormScriptTag</tag-class>
    <body-content>empty</body-content>
    <description>Validates login form.</description>
  </tag>

  <tag>
    <name>inlineScript</name>
    <tag-class>blackboard.servlet.tags.InlineScriptTag</tag-class>
    <body-content>JSP</body-content>
    <description>Inserts inline script into HTML.</description>
    <attribute>
      <name>scriptLanguage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Assumed to be JavaScript, if no input is provided</description>
    </attribute>
    <attribute>
      <name>scriptRef1</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to existing script.</description>
    </attribute>
    <attribute>
      <name>scriptRef2</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to another existing script.</description>
    </attribute>
    <attribute>
      <name>scriptRef3</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to another existing script.</description>
    </attribute>
    <attribute>
      <name>scriptRef4</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Reference to another existing script.</description>
    </attribute>
  </tag>

  <tag>
    <name>spacer</name>
    <tag-class>blackboard.servlet.tags.SpacerTag</tag-class>
    <body-content>empty</body-content>
    <description>
      Creates an invisible area on the page with the specified height and width
    </description>
    <attribute>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Vertical size of the spacer in pixels [1]</description>
    </attribute>
    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Horizontal size of the spacer in pixels [1]</description>
    </attribute>
    <attribute>
      <name>align</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description><![CDATA[<i>deprecated</i>]]></description>
    </attribute>
  </tag>

  <tag>
    <name>devDocTemplate</name>
    <tag-class>blackboard.servlet.tags.DocumentTemplateTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Combines the docTemplateHead and docTemplateBody tag (for use when you don't need to
      add items to the HEAD of the HTML doc
    </description>
    <attribute>
      <name>title</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The page title to be displayed.</description>
    </attribute>
  </tag>

  <tag>
    <name>docTemplate</name>
    <tag-class>blackboard.servlet.tags.ProductionDocTemplateTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Combines the docTemplateHead and docTemplateBody tag (for use when you don't need to
      add items to the HEAD of the HTML doc
    </description>
    <attribute>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The page title to be displayed.</description>
    </attribute>
    <attribute>
      <name>onLoad</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is loaded</description>
    </attribute>
    <attribute>
      <name>onUnload</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is unloaded</description>
    </attribute>
    <attribute>
      <name>onSubmit</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is submitted</description>
    </attribute>
  </tag>

  <tag>
    <name>docTemplateHead</name>
    <tag-class>blackboard.servlet.tags.ProductionDocTemplateHeadTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Sets head defaults, such as including the base css sheet and validateForm.js links.
      Use this tag if you wish to add more style sheets or javascript to the head of your
      HTML doc
    </description>
    <attribute>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The page title to be displayed.</description>
    </attribute>
  </tag>

  <tag>
    <name>docTemplateBody</name>
    <tag-class>blackboard.servlet.tags.ProductionDocTemplateBodyTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Sets the default body tag settings for the HTML page (such as attaching the default
      style class to the body tag)
    </description>
    <attribute>
      <name>onLoad</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is loaded</description>
    </attribute>
    <attribute>
      <name>onUnload</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the page is unloaded</description>
    </attribute>
    <attribute>
      <name>onResize</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Javascript to run when the window is resized</description>
    </attribute>
  </tag>

  <tag>
    <name>coursePage</name>
    <tag-class>blackboard.servlet.tags.CoursePageTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Creates a course page element. It may be used to re-arrange elements on a page from
      interface to interface. It makes the course available to sub-tags
    </description>
    <attribute>
      <name>courseId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Id of the course object</description>
    </attribute>
    <attribute>
      <name>course</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>blackboard.data.course.Course</type>
      <description>
        Seems to be intended to store the actual course object. This attribute is not
        currently used in any pages, but might be in use in an external building block.
      </description>
    </attribute>
  </tag>

  <tag>
    <name>courseTitleBar</name>
    <tag-class>blackboard.servlet.tags.CourseTitlebarTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates a course title bar</description>
  </tag>

  <tag>
    <name>titleBar</name>
    <tag-class>blackboard.servlet.tags.TitlebarTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates a general use title bar.</description>
    <attribute>
      <name>iconUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The URL for the Icon image used in the titlebar</description>
    </attribute>
    <attribute>
      <name>pluginId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The Id for the plugin - known by the plugin developer and used as the directory
        root of the plugin. For instance:
        <![CDATA[<p>]]>
        "/Blackboard/plugins/*pluginId*/images/pluginimg.gif"
      </description>
    </attribute>
  </tag>

  <tag>
    <name>instructionBar</name>
    <tag-class>blackboard.servlet.tags.InstructionbarTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      Creates a general use instruction bar to explain how the page is to be used
    </description>
  </tag>

  <tag>
    <name>step</name>
    <tag-class>blackboard.servlet.tags.StepTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates html for a wizard Step element.</description>
    <attribute>
      <name>number</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The number for the step (1, 2, 3 for 1st, 2nd, 3rd in wizard)
      </description>
    </attribute>
    <attribute>
      <name>title</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The title of the step</description>
    </attribute>
    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The width of the inner (content) table in the Step. Should not normally need to be
        set except in special instances such as when right-aligned content (such as a
        button)
        <![CDATA[<i>]]>
        must
        <![CDATA[</i>]]>
        align at the far right edge of the outer table. Can either be in the format of a
        pixel width ("250") or of a percent ("100%")
      </description>
    </attribute>
  </tag>

  <tag>
    <name>stepSubmit</name>
    <tag-class>blackboard.servlet.tags.StepSubmitTag</tag-class>
    <body-content>empty</body-content>
    <description>Creates html for ane entire submit/cancel step element</description>
    <attribute>
      <name>number</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The number for the step (1, 2, 3 for 1st, 2nd, 3rd in wizard)
      </description>
    </attribute>
    <attribute>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The title of the step</description>
    </attribute>
    <attribute>
      <name>cancelUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The url to recall the page to if the user cancels</description>
    </attribute>
    <attribute>
      <name>instructions</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The message string displayed in this step</description>
    </attribute>
    <attribute>
      <name>submitButton</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        This optional key is used to look up the button and alt text for a nonstandard
        button, such as "Next". usage: submitButton="next". The user must make sure that
        there is a corresponding image and bundle string
        <![CDATA[<p>]]>
        image would be
        \builds\MAINLINE\base\content\locale\en_US\images\ci\formbtns\next_off.gif bundle
        entry would would be tags.properties: button.label.next=Next
      </description>
    </attribute>
  </tag>

  <tag>
    <name>breadcrumbBar</name>
    <tag-class>blackboard.servlet.tags.BreadcrumbBarTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates the framework for the breadcrumb navigation</description>
    <attribute>
      <name>environment</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether we are viewing this navigation in the course, control panel, portal, etc.
        Acceptable values are:
        <![CDATA[<ul>]]>
        <![CDATA[<li>]]>
        PORTAL
        <![CDATA[<li>]]>
        COURSE
        <![CDATA[<li>]]>
        CTRL_PANEL
        <![CDATA[<li>]]>
        SYS_ADMIN
      </description>
    </attribute>
    <attribute>
      <name>handle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The database field used to uniquely identify the root nav item for the breadcrumb
        bar. (bar gets the appropriate nav item from the handler, then traverses the
        navigation tree to iteratively get the item's parents
      </description>
    </attribute>
    <attribute>
      <name>tabId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The top-level origin tab ID. If set, in conjunction with the PORTAL environment,
        prepends a crumb for the parent tab to the crumb provided by getHandle
      </description>
    </attribute>
    <attribute>
      <name>isContent</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Indicates whether this navigation bar should render content components. If set,
        the tag will use context information to * display the "content" portion of the
        breadcrumb
      </description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The default target for bc items in this bar, defaults to none
      </description>
    </attribute>
    <attribute>
      <name>breadcrumbs</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.util.Collection</type>
      <description>You can also pass in a collection of Breadcrumb objects</description>
    </attribute>
    <attribute>
      <name>mode</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Enables the display view. Acceptable value is: ENABLE</description>
    </attribute>
  </tag>

  <tag>
    <name>breadcrumb</name>
    <tag-class>blackboard.servlet.tags.BreadcrumbTag</tag-class>
    <body-content>JSP</body-content>
    <description>Navigation item for the breadcrumb bar.  Body of this tag should contain the text to display for this breadcrumb</description>
    <attribute>
      <name>href</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The optional attribute setting the link for the element</description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The target attribute for the links in this breadcrumb, defaults to none
      </description>
    </attribute>
  </tag>

  <tag>
    <name>dataElement</name>
    <tag-class>blackboard.servlet.tags.DataElementTag</tag-class>
    <body-content>JSP</body-content>
    <description>This tag will be superceded by type-safe form input tags</description>
    <attribute>
      <name>label</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The text label for the data (form) element; should contain: form input tags
      </description>
    </attribute>
    <attribute>
      <name>required</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Indicates that the element is required before submitting form (Doesn't validate,
        simply adds *)
      </description>
    </attribute>
    <attribute>
      <name>isVertical</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Indicates that the label and content should be vertically aligned (label on top of
        element instead of to left of element)
      </description>
    </attribute>
  </tag>

  <tag>
    <name>dataSubElement</name>
    <tag-class>blackboard.servlet.tags.DataSubElementTag</tag-class>
    <body-content>JSP</body-content>
    <description>This tag will be superceded by type-safe form input tags</description>
    <attribute>
      <name>label</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The text label for the data (form) element. Should contain: form input tags
      </description>
    </attribute>
    <attribute>
      <name>required</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Indicates that the element is required before submitting form (Doesn't validate,
        simply adds *)
      </description>
    </attribute>
  </tag>

  <tag>
    <name>instructions</name>
    <tag-class>blackboard.servlet.tags.InstructionsTag</tag-class>
    <body-content>JSP</body-content>
    <description>Instructions should only be called inside a step</description>
  </tag>

  <tag>
    <name>stepContent</name>
    <tag-class>blackboard.servlet.tags.StepContentTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      StepContent should only be called inside a step. It wraps the enclosed HTML in the
      appropriate table row and table data code to fit inside the step tag. It is
      comparable to the specialized tags Instructions and DataElement, but is more
      generalized since it ONLY contains table code
    </description>
  </tag>

  <tag>
    <name>button</name>
    <tag-class>blackboard.servlet.tags.ButtonTag</tag-class>
    <body-content>empty</body-content>
    <description>
      This tag will be superceded by type-safe button tags. It hides the details of a
      button object (eventually will be functional in javascript and non-javascript modes)
    </description>
    <attribute>
      <name>type</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether the buton is FORM_ACTION, INLINE, LONG_INLINE, or TOOLBAR
        <![CDATA[<p>]]>
        FORM_ACTION corresponds to the "submit"|"cancel" button type
        <![CDATA[<br>]]>
        ACTION_INLINE corresponds to the "modify"|"remove" button type
        <![CDATA[<br>]]>
        LONG_INLINE corresponds to inline buttons that don't have the standard button size
        (e.g., "properties")
        <![CDATA[<br>]]>
        TOOLBAR corresponds to the toolbar buttons (e.g. "add item") BUT NOT buttons used
        in actionbars (only old style toolbar buttons)
      </description>
    </attribute>
    <attribute>
      <name>name</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The name of the button, which should correspond to the name of the image for the
        button, minus the "_off.gif"
      </description>
    </attribute>
    <attribute>
      <name>alt</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Used for the alt tag for the code</description>
    </attribute>
    <attribute>
      <name>label</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        NEW inline button behavior: the label will display for text buttons; if it is
        empty, use the alt instead
      </description>
    </attribute>
    <attribute>
      <name>action</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Whether the button submits a form (SUBMIT_FORM) or links to another page (LINK)
      </description>
    </attribute>
    <attribute>
      <name>targetUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The target for the page to link to, if it's a link-type button
      </description>
    </attribute>
    <attribute>
      <name>target</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The (optional) frame name for the link to open in, if it's a link-type button and
        "_self" is not adequate
      </description>
    </attribute>
    <attribute>
      <name>tabindex</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Corresponds to the html tabindex property, used to set the tab order of a given
        link or button
      </description>
    </attribute>
    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Pixel width of the button (will be deduced from 'type' if not supplied)
      </description>
    </attribute>
    <attribute>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Pixel height of the button (will be deduced from 'type' if not supplied)
      </description>
    </attribute>
    <attribute>
      <name>tabId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        If the parent is a tab, remeber the tab id for OK button navigation
      </description>
    </attribute>
  </tag>

  <tag>
    <name>okButton</name>
    <tag-class>blackboard.servlet.tags.OkButtonTag</tag-class>
    <body-content>empty</body-content>
    <description>
      This is a type-safe tag wrapping the ButtonTag for OK buttons. It can return to the
      parent nav item page by default, or take a targetUrl
    </description>
    <attribute>
      <name>targetUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The target for the page to link to, if it's a link-type button
      </description>
    </attribute>
    <attribute>
      <name>tabindex</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Corresponds to the html tabindex property, used to set the tab order of a given
        link or button
      </description>
    </attribute>
  </tag>

  <tag>
    <name>receipt</name>
    <tag-class>blackboard.servlet.tags.ReceiptTag</tag-class>
    <body-content>JSP</body-content>
    <description>Creates a receipt page using titleBar and button components</description>
    <attribute>
      <name>type</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Type of receipt. Valid values: (SUCCESS, FAIL, DISABLED). If type is not set to a
        supported value or is null, then default to successful receipt
      </description>
    </attribute>
    <attribute>
      <name>iconUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        A String containing the iconUrl. Should look like:
        "/images/ci/icons/receiptsuccess_u.gif"
      </description>
    </attribute>
    <attribute>
      <name>pluginId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
